ServerAliveInterval 60
ForwardX11 yes

Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_github
    IdentityFile ~/.ssh/id_rsa

Host *.rz-amper.de
    SendEnv LANG LC_*
    KexAlgorithms +diffie-hellman-group1-sha1,diffie-hellman-group14-sha1,diffie-hellman-group-exchange-sha1
    Ciphers +aes256-cbc
